@article{GT,
  abbr = {Georgia Tech},
  title={Mars Rover Vehicle Routing Optimization},
  Month = {},
  subtitle = {Space Systems Optimization Group},
  year={2025},
  publisher={GT},
  preview={ssog.jpg},
  abstract = {As part of the Space Systems Optimization Group at Georgia Tech,
   I conduct research on optimizing Mars rover surface routing to support energy-efficient and autonomous exploration. 
   The core of this work involves formulating a Mixed-Integer Linear Programming (MILP) model that incorporates elevation-based terrain costs to minimize energy. 
   To ensure feasible and continuous paths, I implemented subtour elimination constraints that prevent disconnected or looping routes. I am extending this model to account for the stochastic nature of orbital satellite data, which introduces uncertainty in terrain information. This involves developing adaptive planning techniques that allow the rover to update its route based on newly observed conditions (local routing/global routing). The overall objective is to improve mission robustness and operational autonomy for long-duration planetary exploration.},
  selected = {true},
  appear = {true}
}

